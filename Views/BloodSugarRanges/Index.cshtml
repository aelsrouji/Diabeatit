@model IEnumerable<Diabeatit.Models.BloodSugarRange>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.BloodSugarRangeFrom)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.BloodSugarRangeTo)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.BloodSugarRangeFrom)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.BloodSugarRangeTo)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.BloodSugarRangeId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.BloodSugarRangeId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.BloodSugarRangeId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
